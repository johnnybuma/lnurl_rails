{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@hotwired/stimulus": "^3.0.1",
    "@hotwired/turbo-rails": "^7.1.1",
    "@popperjs/core": "^2.11.2",
    "@rails/actioncable": "^6.1.5",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "c3": "^0.7.20",
